<div class="container-fluid">
    <div class="row">
        <div class="col-md-offset-3 col-md-6">
            <% for(var i=0; i < scholarships.length; i++) { %>
            <div class="panel panel-default" style="border-radius: 0px">
                <div class="panel panel-heading">
                    <div class="panel-title">
                        <strong>
                            <%= scholarships[i].name %>
                        </strong>
                    </div>
                
                </div>

                <div class="panel panel-body">
                    <div id="scholarship_id" style="display:none"><%= scholarships[i].scholarship_id%></div>
                    <div class="table-responsive">
                        <table class="table table-hover">
                            <tr>
                                <td><strong>Prerequisite</strong></td>
                                <td> <%= scholarships[i].prerequisite %></td>
                            </tr>
                            <tr>
                                <td><strong>Type</strong></td>
                                <td> <%= scholarships[i].type %></td>
                            </tr>
                            <tr>
                                <td><strong>Amount</strong></td>
                                <td> $<%= scholarships[i].amount %></td>
                            </tr>
                            <tr>
                                <td><strong>Expires</strong> </td>
                                <td> <%= scholarships[i].expiry %></td>
                            </tr>
                        </table>
                    </div>
                    <div class="text-center">
                        <button type="submit" onclick="openApplyModal(this)" class="btn btn-primary">Apply</button>
                    </div>
                </div>
            </div>
            <% } %>
        </div>

    </div>
</div>
