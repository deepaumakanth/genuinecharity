<div id="signinModal" class="modal fade" role="dialog">
    <div class="modal-dialog">

        <!-- Modal content-->
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                <h4 class="modal-title">Sign In</h4>
            </div>
            <div class="modal-body">
                <div class="form-group">
                    <label for="emailsignup">Email ID</label>
                    <input type="text" class="form-control" id="emailsignup" name="emailsignup" placeholder="Enter your email id" required/>
                </div>
                <div class="form-group">
                    <label for="passwordsignup">Password</label>
                    <input type="password" class="form-control" id="passwordsignup" name="passwordsignup" placeholder="Enter your password" required/>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-success" onclick="signinSubmit()" >Submit</button>
                <button type="button" class="btn btn-danger" data-dismiss="modal">Close</button>
            </div>
        </div>

    </div>
</div>

<div id="applyModal" class="modal fade" role="dialog">
    <div class="modal-dialog">
        <!-- Modal content-->
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                <h4 class="modal-title">Apply to Scholarship</h4>
            </div>
            <div class="modal-body">
                <div class="form-group">
                    <label for="noofchildren">No. Of children</label>
                    <input type="noofchildren" class="form-control" id="noofchildren" name="noofchildren">
                </div>
                 <label>Education:</label>
                  <div class="form-group">
                    <label for="education">Highest Education</label>
                    <select class="form-control" id="education" name="education" placeholder="Enter highest education details">
                        <option>Elementary/Primary</option>
                        <option>Middle/High</option>
                        <option>High school</option>
                        <option>University</option>
                    </select>
                </div>
                <div class="form-group">
                    <label for="edyear">Year</label>
                    <input type="text" class="form-control" id="edyear" name="edyear">
                </div>
                 <div class="form-group">
                    <label for="edschool">School</label>
                    <input type="text" class="form-control" id="edschool" name="edschool">
                </div>
                <div class="form-group">
                    <label for="edcity">City</label>
                    <input type="text" class="form-control" id="edcity" name="edcity">
                </div>
                <div class="form-group">
                    <label for="edcountry">Country</label>
                    <input type="text" class="form-control" id="edcountry" name="edcountry">
                </div>
                 <div class="form-group">
                    <label for="edclass">Class</label>
                    <input type="text" class="form-control" id="edclass" name="edclass">
                </div>
                 <div class="form-group">
                    <label for="edgrade">Grade</label>
                    <input type="text" class="form-control" id="edgrade" name="edgrade">
                </div>
                <div class="form-group">
                    <label for="edrank">Rank</label>
                    <input type="text" class="form-control" id="edrank" name="edrank">
                </div>

                <div class="form-group">
                    <label for="eddegree">Degree</label>
                    <input type="text" class="form-control" id="eddegree" name="eddegree">
                </div>

                <label>List your whereabouts during the hollow year:</label>
                <div class="form-group">
                    <label for="hollowyear">Hollow year</label>
                    <input type="text" class="form-control" id="hollowyear" name="hollowyear">
                </div>
                <div class="form-group">
                    <label for="activities">Hollow year activities</label>
                    <input type="textarea" class="form-control" id="activities" name="activities">
                </div>
                <div class="form-group">
                    <label for="termination">Have you ever been terminated from school ?</label>
                    <br><input type="radio" id="terminated" name="termination" value="Yes"> Yes</label><br>
                    <input type="radio" id="terminated" name="termination" value="No"> No</label>
                </div>
                <label>Information about Parents:</label>
                <div class="form-group">
                    <label for="parentsms">Parent's Marital Status</label>
                    <select class="form-control" id="parentsms" name="parentsms" placeholder="Enter marital status">
                        <option>unmarried</option>
                        <option>married</option>
                        <option>separated</option>
                        <option>divorced</option>
                        <option>widow</option>
                    </select>
                </div>

                <div class="form-group">
                    <label for="address">Father's Name</label>
                    <input type="text" class="form-control" id="fathername" name="fathername" placeholder='Enter your fathers first and last name' required/>
                </div>
                <div class="form-group">
                    <label for="address">Father's age</label>
                    <input type="text" class="form-control" id="fatherage" name="fatherage" placeholder='Enter your fathers age' required/>
                </div>
               
                <div class="form-group">
                    <label for="mothername">Mother's Name</label>
                    <input type="text" class="form-control" id="mothername" name="mothername" placeholder='Enter your mother first and last name' required/>
                </div>
                <div class="form-group">
                    <label for="motherage">Mother's age</label>
                    <input type="text" class="form-control" id="motherage" name="motherage" placeholder='Enter your mothers age' required/>
                </div>
                <div class="form-group">
                    <label for="address">Phone</label>
                    <input type="text" class="form-control" id="phone" name="Phone"  required/>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-success" onclick="applyScholarship()" >Submit</button>
                <button type="button" class="btn btn-danger" data-dismiss="modal">Close</button>
            </div>
        </div>
    </div>
</div>

